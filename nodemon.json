{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "test/fixtures/",
    "test/samples/",
    "public/*.html",
    "public/css/*.*",
    "public/js/*.*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}